<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>KornSW.DynamicProxy</id>
    <version>1.2.0-localbuild</version>
    <title>KornSW.DynamicProxy</title>
    <authors>KornSW</authors>
    <owners>KornSW</owners>
    <copyright>KornSW</copyright>
    <description>generates proxy-classes for given interfaces via EMIT which are redirecting every call to a background hook</description>
    <tags>dynamic proxy EMIT runtime</tags>
    <!-- https://spdx.org/licenses/ -->
    <license type="expression">GPL-3.0-only</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <icon>PackageIcon.png</icon>
    <projectUrl>https://github.com/KornSW/DynamicProxy</projectUrl>
    <dependencies>
      <group targetFramework=".NETFramework4.8">
      </group>
      <group targetFramework="net8.0">
        <dependency id="System.Reflection.Emit" version="4.7.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- https://docs.microsoft.com/en-us/nuget/create-packages/creating-a-package#from-a-convention-based-working-directory -->
    <file src="PackageIcon.png" target="" />
    <!-- .NET 4.8 -->
    <file src="(Stage)\Bin\net48\KornSW.DynamicInterfaceProxy.dll" target="lib/net48" />
    <file src="(Stage)\Bin\net48\KornSW.DynamicInterfaceProxy.xml" target="lib/net48" />
    <file src="(Stage)\Bin\net48\KornSW.DynamicInterfaceProxy.pdb" target="lib/net48" />
    <!-- .NET 8.0 -->
    <file src="(Stage)\Bin\net8.0\KornSW.DynamicInterfaceProxy.dll" target="lib/net8.0" />
    <file src="(Stage)\Bin\net8.0\KornSW.DynamicInterfaceProxy.xml" target="lib/net8.0" />
    <file src="(Stage)\Bin\net8.0\KornSW.DynamicInterfaceProxy.pdb" target="lib/net8.0" />
  </files>
</package>
